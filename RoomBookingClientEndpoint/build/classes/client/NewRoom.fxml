<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="212.0" prefWidth="243.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.NewRoomController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" layoutY="4.0" prefHeight="324.0" prefWidth="376.0">
         <children>
            <Label text="Add a new Room: ">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <GridPane prefHeight="157.0" prefWidth="285.0">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Room Name:" />
                  <Label text="Room Type:" GridPane.rowIndex="1" />
                  <Label text="Number of seats:" GridPane.rowIndex="2" />
                  <TextField fx:id="roomName" GridPane.columnIndex="1" />
                  <TextField fx:id="roomType" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="numbSeats" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Button fx:id="submit" mnemonicParsing="false" onAction="#createRoom" text="Submit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
