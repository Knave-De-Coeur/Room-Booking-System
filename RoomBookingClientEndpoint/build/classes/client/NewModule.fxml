<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="254.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.NewModuleController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="377.0" prefWidth="452.0">
         <children>
            <Label contentDisplay="CENTER" text="Add a New Module:">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <GridPane alignment="CENTER" prefHeight="267.0" prefWidth="339.0" VBox.vgrow="NEVER">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label contentDisplay="CENTER" text="Module Name:" />
                  <Label text="Module Description:" GridPane.rowIndex="1" />
                  <Label text="Module Leader:" GridPane.rowIndex="2" />
                  <Label text="Lecturer's Username:" GridPane.rowIndex="3" />
                  <TextArea fx:id="moduleDesc" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="moduleName" prefHeight="25.0" prefWidth="99.0" GridPane.columnIndex="1" />
                  <TextField fx:id="Lecturer" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="moduleLeader" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Button fx:id="submit" mnemonicParsing="false" onAction="#createModule" text="Submit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
